	<import>
		<!-- tracker button -->
		<templatelistener name="userTrackerButton">
			<environment>user</environment>
			<templatename>user</templatename>
			<eventname>contentHeaderNavigation</eventname>
			<templatecode><![CDATA[{include file='__trackerUserButton'}]]></templatecode>
		</templatelistener>
		
		<!-- tracker button JS -->
		<templatelistener name="trackerJavascriptInclude">
			<environment>user</environment>
			<templatename>user</templatename>
			<eventname>javascriptInclude</eventname>
			<templatecode><![CDATA[{include file='__trackerUserJavascriptInclude'}]]></templatecode>
		</templatelistener>
	</import>
</data>